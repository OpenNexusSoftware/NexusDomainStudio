{
    "id": "change_depot_external_ref",
    "name": "ChangeDepotExternalReference",
    "args": [
        { "name": "externalRef", "type": "external_reference", "required": true, "description": "The external id of the depot provider" }
    ],
    "emits": [
        {
            "event": "depot_external_id_changed",
            "description": "Emitted when a depot's external reference is changed.",
            "maps": [
                { "property": "depotId", "value": "$this.id" },
                { "property": "externalRef", "value": "$this.extrernalRef" }
            ]
        }
    ]
}